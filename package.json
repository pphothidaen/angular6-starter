{
  "name": "starter-kit",
  "version": "0.0.0",
   "stylelint": {
     "plugins": [
       "stylelint-config-standard", "stylelint-config-recommended-scss","stylelint-no-unsupported-browser-features"
     ],
     "rules": {
       "plugin/no-unsupported-browser-features": [true, {
         "browsers": ["> 0.5%", "last 2 versions", "Firefox ESR", "not dead"],
         "ignore": ["rem"],
         "severity": "warning"
       }]
     },
   "defaultSeverity": "warning"
 },
